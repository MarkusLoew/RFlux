Package: RFlux
Type: Package
Title: Eddy Covariance Flux Data Processing
Version: 1.0.1
Date: 2019-09-19
Authors@R: 
  c(person(given = "Domenico",
           family = "Vitale",
           role = c("aut", "cre"),
           email = "domvit@unitus.it"),
    person(given = "Dario",
           family = "Papale",
